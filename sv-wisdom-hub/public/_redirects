import { BrowserRouter, Routes, Route } from "react-router-dom";
import Home from "./pages/home";
import Admin from "./pages/admin/index";   // index.jsx file

function App() {
  return (
    <BrowserRouter>
      <Routes>
        <Route path="/" element={<Home />} />
        <Route path="/admin" element={<Admin />} />   {/* Capital A */}
      </Routes>
    </BrowserRouter>
  );
}

export default App;
